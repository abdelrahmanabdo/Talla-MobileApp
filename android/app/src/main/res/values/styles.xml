<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">#012647</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:textColorAlertDialogListItem">#012647</item>
        <item name="android:windowActionBar">True</item>
    </style>
    <style name="SplashTheme" parent="SplashScreen_SplashTheme">
        <item name="android:windowBackground">@drawable/launch_screen</item>
        <item name="android:windowActionBar">True</item>
        <item name="android:statusBarColor">#012647</item>
        <item name="android:textColorAlertDialogListItem">#0C9EBF</item>
    </style>
    
</resources>